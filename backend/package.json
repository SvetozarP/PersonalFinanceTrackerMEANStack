{
  "name": "finance-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Personal Finance Tracker",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --config jest.config.coverage.js --coverage",
    "test:ci": "jest --config jest.config.coverage.js --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "format:check": "prettier --check src/**/*.{ts,js,json}",
    "type-check": "tsc --noEmit",
    "optimize-db": "ts-node src/scripts/optimize-database.ts",
    "validate-performance": "ts-node src/scripts/validate-performance.ts",
    "mock-data": "ts-node scripts/mock-data.ts",
    "mock-data:insert": "ts-node scripts/mock-data.ts insert",
    "mock-data:clear": "ts-node scripts/mock-data.ts clear",
    "mock-data:reset": "ts-node scripts/mock-data.ts both",
    "superuser": "ts-node scripts/create-superuser.ts",
    "superuser:create": "ts-node scripts/create-superuser.ts create",
    "superuser:list": "ts-node scripts/create-superuser.ts list",
    "user:delete": "ts-node scripts/delete-user.ts",
    "user:list": "ts-node scripts/delete-user.ts list",
    "user:delete-all": "ts-node scripts/delete-user.ts delete-all"
  },
  "keywords": [
    "finance",
    "tracker",
    "api",
    "express",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.96",
    "@types/mongoose-unique-validator": "^1.0.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "mongoose-unique-validator": "^5.0.0",
    "morgan": "^1.10.1",
    "nestjs": "^0.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
